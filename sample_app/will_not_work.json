{
  "app_name": "temp monitor",
  "app_description": "A cool app to know when it is going to be cool and when it is going to be hot",
  "has_dockerfile": true,
  "custom_sensors": {
    "Temperature": 1,
    "Humidity": 0,
    "Luminosity": 1,
    "Power": 0,
    "Presence": 1,
    "Lamp": 2,
    "Buzzer": 0
  }, // locations with availability is given
  "custom_type": ["Temperature", "Humidity", "Luminosity"], // all sensors measured these will be given
  "custom_location": ["H-105", "H-205"], // all sensors in these rooms will be given
  "iudx_sensors": ["Temperature", "PM2.5", "PM10"], // apis given by suhas, based on parameters, end user will; select the nodes
  
}
